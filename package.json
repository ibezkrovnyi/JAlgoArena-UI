{
  "name": "jalgoarena-ui",
  "version": "2.4.0",
  "description": "JAlgoArena UI",
  "main": "build/server.js",
  "scripts": {
    "installRequiredGlobalDependencies": "npm install webpack-cli -g",
    "pretest": "./node_modules/.bin/flow check",
    "test": "jest && codecov",
    "test:watch": "jest --watch",
    "build": "webpack --config webpack.server.config.js --mode=production && webpack --config webpack.client.config.js --mode=production",
    "prestart": "npm run build",
    "start": "node server.js",
    "predeploy": "npm run build",
    "deploy": "node createReleasePackage.js",
    "client": "webpack-dev-server --mode=development --devtool inline-source-map --hot --inline",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jalgoarena/JAlgoArena-UI.git"
  },
  "keywords": [
    "JAlgoArena"
  ],
  "author": "Jacek Sp√≥lnik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jalgoarena/JAlgoArena-UI/issues"
  },
  "homepage": "https://github.com/jalgoarena/JAlgoArena-UI#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0-10",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.8",
    "@stomp/stompjs": "^4.0.2",
    "ajv": "^6.4.0",
    "archiver": "^2.1.1",
    "bootstrap": "^3.3.7",
    "brace": "^0.11.1",
    "connected-react-router": "^4.3.0",
    "enzyme": "^3.3.0",
    "highcharts": "^6.1.1",
    "highcharts-react-official": "^1.3.1",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "popper.js": "^1.14.3",
    "react": "^16.3.2",
    "react-ace": "^5.10.0",
    "react-addons-create-fragment": "^15.6.2",
    "react-blockies": "^1.3.0",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-table": "^4.3.1",
    "react-calendar-heatmap": "^1.6.3",
    "react-datetime-bootstrap": "^1.4.5",
    "react-dom": "^16.3.2",
    "react-json-tree": "^0.11.0",
    "react-mixins": "^0.1.6",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1",
    "redbox-react": "^1.6.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "remarkable": "^1.7.1",
    "s": "^0.1.1",
    "sockjs-client": "^1.1.4",
    "spin.js": "^3.1.0",
    "typeface-roboto": "0.0.54",
    "serve-static": "^1.13.2",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "morgan": "^1.9.0",
    "compression": "^1.7.2",
    "http-proxy-middleware": "^0.18.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.69.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-root-element-plugin": "^0.1.3",
    "jest": "^23.1.0",
    "jest-fetch-mock": "^1.5.0",
    "jest-localstorage-mock": "^2.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redux-mock-store": "^1.2.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-parse": "latest",
    "webpack": "^4.8.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "automock": false,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "setupFiles": [
      "jest-localstorage-mock",
      "./setupJest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "spin.js$": "<rootDir>/__mocks__/objMock.js"
    }
  }
}
