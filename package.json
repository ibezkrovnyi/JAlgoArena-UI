{
  "name": "jalgoarena",
  "version": "2.0.0",
  "description": "JAlgoArena UI",
  "main": "server.js",
  "scripts": {
    "installRequiredGlobalDependencies": "npm install webpack -g & npm install pm2 -g",
    "test": "jest && codecov",
    "pretest": "./node_modules/.bin/flow check",
    "test:watch": "jest --watch",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node server.js",
    "prepm2start": "npm run build",
    "pm2start": "NODE_ENV=production pm2 start server.js --name jalgoarena-ui",
    "preherokuStart": "npm run herokuBuild",
    "herokuStart": "NODE_ENV=production node server.js",
    "dev": "node server.js --trace-sync-io",
    "prod": "NODE_ENV=production node server.js",
    "prebuild": "node deletePublicDir.js",
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "herokuBuild": "NODE_ENV=production ./node_modules/.bin/webpack --progress --config webpack.config.prod.js",
    "predeploy": "npm run build",
    "deploy": "node createReleasePackage.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spolnik/JAlgoArena-UI.git"
  },
  "keywords": [
    "JAlgoArena"
  ],
  "author": "Jacek Sp√≥lnik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spolnik/JAlgoArena-UI/issues"
  },
  "homepage": "https://github.com/spolnik/JAlgoArena-UI#readme",
  "dependencies": {
    "@stomp/stompjs": "^4.0.2",
    "archiver": "^2.1.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "brace": "^0.11.1",
    "compression": "^1.7.2",
    "cp-file": "^5.0.0",
    "del": "^3.0.0",
    "enzyme": "^3.3.0",
    "express": "^4.16.3",
    "helmet": "^3.12.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "react": "^16.3.1",
    "react-ace": "^5.10.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^16.3.1",
    "react-json-tree": "^0.11.0",
    "react-mixins": "^0.1.6",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "react-router-bootstrap": "^0.23.3",
    "react-router-redux": "^4.0.8",
    "react-spin": "^0.6.2",
    "redbox-react": "^1.5.0",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-filter-actions": "^1.2.2",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-mock-store": "^1.2.2",
    "redux-thunk": "^2.2.0",
    "remarkable": "^1.7.1",
    "s": "^0.1.1",
    "serve-static": "^1.13.2",
    "sockjs-client": "^1.1.4",
    "webpack": "^4.5.0",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.0"
  },
  "devDependencies": {
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "flow-bin": "^0.69.0",
    "jest": "^22.4.3",
    "nock": "^9.2.3",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack-cli": "^2.0.14"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
